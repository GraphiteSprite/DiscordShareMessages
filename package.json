{
  "name": "discordtotelegram",
  "version": "1.3.1",
  "description": "Simple app that take messages from a Discord Server and send them to a Telegram channel/group",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -k -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,green\" \"tsc --watch\" \"nodemon dist/index.js\"",
    "start": "tsc && node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Surfict/DiscordToTelegram.git"
  },
  "author": "Surfict",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Surfict/DiscordToTelegram/issues"
  },
  "homepage": "https://github.com/Surfict/DiscordToTelegram#readme",
  "dependencies": {
    "@types/i18next": "^11.9.3",
    "@types/node-telegram-bot-api": "^0.30.3",
    "discord.js": "^11.4.2",
    "i18next": "^12.0.0",
    "moment": "^2.22.2",
    "node-telegram-bot-api": "^0.30.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.1",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.5",
    "typescript": "^3.1.4"
  }
}
